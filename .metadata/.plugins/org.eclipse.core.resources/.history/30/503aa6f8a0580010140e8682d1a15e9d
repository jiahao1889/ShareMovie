<struts>
	<package name="ShareMovie" namespace="/" extends="struts-default">
		<action name="login" class="com.sysu.sharemovie.action.user.Login">
		<result name="success" type="redirectAction">home</result>
		<result name="input">login.jsp</result>
		</action>
		<action name="register" class="com.sysu.sharemovie.action.user.Register">
		<result name="success" type="redirectAction">home</result>
		<result name="input">register.jsp</result>
		</action>
		<action name="*">
		<result>login.jsp</result>
		</action>
	</package>
</struts>